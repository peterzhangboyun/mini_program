<template name='searchlist'>
  <block wx:for='{{array}}'>
    <view class='item' bindtap='seeDetail' id='0'>
     
      <view>
        <image src='{{item.img}}' style='width:75px;height:58px;'></image>  
      </view>
     
      <view class='desc'>   
        <view class='title'>{{item.title}}</view>
        <view class='count'>
          <view>{{item.type}}</view>
          <view class='content'>{{item.content}}</view>
          <view class='Deadline'>{{item.Deadline}}</view>
        </view>
      </view>
    </view>
  <view class='hr2'></view>
  </block>

</template>